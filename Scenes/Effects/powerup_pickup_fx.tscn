[sub_resource type="Shader" id="Shader_ufk1i"]
code = "shader_type canvas_item;

uniform float progress : hint_range(0.0, 1.0, 0.001);
const float MIN = 6.0;

void fragment() {
	vec2 pos = UV - vec2(0.5, 0.5);
	
	float v = abs(abs(pos.x) > abs(pos.y) ? pos.y / pos.x : pos.x / pos.y);
	if ((v > 0.15 && v < 0.4) || (v > 0.6 && v < 0.8)) {
		COLOR.a = 0.0;
	} else {
		COLOR.a *= max(0.0, progress - length(pos) * MIN * progress * progress);
	}
}"

