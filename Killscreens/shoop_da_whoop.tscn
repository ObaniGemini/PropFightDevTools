[sub_resource type="Shader" id="Shader_jk5a3"]
code = "shader_type canvas_item;

uniform float MOVE_FACTOR: hint_range(0.0, 1.0, 0.001);

const float FACTOR = 4.0;
const float INV_FACTOR = 1.0/(FACTOR * 2.0);

void fragment() {
	float t = TIME * 40.0;
	
	vec2 pos = vec2(UV.x * FACTOR - 0.5, (UV.y - 0.5) * (1.0 + MOVE_FACTOR * 2.0) + (cos(t + UV.x * 20.0)) * (sqrt(max(0.0, UV.x - 0.025)) * MOVE_FACTOR));
	if (UV.x <= INV_FACTOR && 1.0 - length(pos) < 0.5) {
		COLOR.a = 0.0;
	} else if (abs(pos.y) > 0.5) {
		COLOR.a = 0.0;
	}
	
	
	float grad = clamp(UV.x * 30.0 - abs(pos.y), 0.0, 1.0);
	COLOR.a *= min(1.0, grad * grad * grad);
	COLOR.rgb *= grad * grad * grad;
}
"

