; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PropFightDevTools"
run/main_scene="res://ModTester.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="PropFight"
config/features=PackedStringArray("4.3")
run/low_processor_mode=true
boot_splash/bg_color=Color(0, 0, 0, 1)

config/icon="res://icon.png"
boot_splash/minimum_display_time=250

[autoload]

util="*res://Scenes/Main/util.gd"
global="*res://Scenes/Main/global.gd"
controls="*res://Scenes/Main/controls.gd"
game="*res://Scenes/Main/game.gd"
environment="*res://Scenes/Main/environment.tscn"
config="*res://Scenes/Main/config.gd"
camera="*res://Scenes/Main/camera.tscn"
color="*res://Scenes/Main/color.gd"
debug="*res://Scenes/Main/debug.tscn"

[debug]

gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false
gdscript/warnings/narrowing_conversion=false
shapes/collision/shape_color=Color(0, 0.6, 0.701961, 0)

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/ropesim/plugin.cfg", "res://addons/very-simple-twitch/plugin.cfg")

[filesystem]

import/blender/enabled=false

[global_group]

nojump=""

[gui]

theme/custom_font="res://Data/Fonts/OpenSans-Semibold96.tres"

[importer_defaults]

animation_library={
"animation/fps": 30.0
}

